<script>

var dateId = ['#fr_work_dta87ff679a2f3e71d9181a67b7542122c','#to_work_dta87ff679a2f3e71d9181a67b7542122c'];
if(dateId.length > 1){
	$.func.selectPeriod('1m', $(dateId[0]), $(dateId[1]));
}
var calOption = {inputId: dateId};


if(dateId.length > 0){
	$(dateId[0]).closest('dd').children('.cal-area').attr('data-init-period','1m');
}

var paramData = {code : $.otpCode('calendar', {'bld' : '/COM/market_date_nt'})};
$.func.ajaxSubmit('/contents/MKD/99/MKD99000001.jspx', paramData, false).success(function(data){
	var objData = data[$.func.getJsonKey(data,0)][0];
	var dateData = objData[$.func.getJsonKey(objData,0)];
	
	if(dateId.length > 0){
		$.each(dateId, function(i, item){
			
			$(item).val(dateData);
